package player;

public class Frame
{
    private int[] bowl;
    private int result;
    
    Frame()
    {
        bowl = new int[2];
    }
    
    public void SetBowl(int bowlTurn, int bowlResult)
    {
        bowl[bowlTurn-1] = bowlResult;
    }
    
    public void SetResult(int frameResult)
    {
        result = frameResult;
    }
    
    public int GetBowl(int bowlTurn)
    {
        return bowl[bowlTurn-1];
    }
    
    public int GetResult()
    {
        return result;
    }
}