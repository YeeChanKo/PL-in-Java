package player;

public class Frame
{
    protected int[] bowlResult;
    protected int frameResult;
    
    Frame()
    {
        bowlResult = new int[2];
    }
    
    public void SetBowlResult(int bTurn, int bResult)
    {
        bowlResult[bTurn-1] = bResult;
    }
    
    public void SetFrameResult(int fResult)
    {
        frameResult = fResult;
    }
    
    public int GetBowlResult(int bTurn)
    {
        return bowlResult[bTurn-1];
    }
    
    public int GetFrameResult()
    {
        return frameResult;
    }
}