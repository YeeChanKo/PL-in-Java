package player;

public class Frame
{
    protected int[] bowlResult;
    protected int frameResult;
    
    Frame()
    {
        bowlResult = new int[2];
    }
    
    public void SetBowl(int bTurn, int bResult)
    {
        bowlResult[bTurn-1] = bResult;
    }
    
    public void SetResult(int fResult)
    {
        frameResult = fResult;
    }
    
    public int GetBowl(int bTurn)
    {
        return bowlResult[bTurn-1];
    }
    
    public int GetResult()
    {
        return frameResult;
    }
}